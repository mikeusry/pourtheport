			.hero-content h1{font-size:clamp(2rem,5vw,3.5rem);font-weight:800;color:#1f2937;margin-bottom:1rem}
			.hero-content p{font-size:1.25rem;color:#4b5563;margin-bottom:2rem}
			.cta-button{display:inline-block;background:#317D40;color:white;padding:1rem 2rem;border-radius:0.5rem;text-decoration:none;font-weight:600;font-size:1.125rem}
			.hero-image{position:relative;max-width:100%;height:auto}
			.hero-image img{width:100%;height:auto;border-radius:1rem;box-shadow:0 25px 50px -12px rgba(0,0,0,0.25)}
			.trust-badge{position:absolute;top:-1rem;right:-1rem;background:#317D40;color:white;padding:0.5rem 1rem;border-radius:9999px;font-size:0.875rem;font-weight:700;box-shadow:0 10px 25px -5px rgba(0,0,0,0.1)}
			@media (max-width:768px){
				.hero-container{grid-template-columns:1fr;text-align:center}
				.hero-image{order:-1;max-width:300px;margin:0 auto}
			}
			/* Prevent layout shift */
			img[width][height]{aspect-ratio:attr(width)/attr(height)}
			/* Optimize animations */
			@media (prefers-reduced-motion:reduce){*{animation:none!important;transition:none!important}}
			
			/* Screen reader utilities */
			.sr-only{position:absolute;width:1px;height:1px;padding:0;margin:-1px;overflow:hidden;clip:rect(0,0,0,0);white-space:nowrap;border:0}
			.focus\:not-sr-only:focus{position:static;width:auto;height:auto;padding:initial;margin:initial;overflow:visible;clip:auto;white-space:normal}
		</style>
		
		
